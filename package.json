{
  "name": "react-typescript-eslint-prettier-boilerplate",
  "author": {
    "name": "Michiel Bouw",
    "url": "http://michielbouw.nl"
  },
  "license": "MIT",
  "version": "0.0.2",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "eslint --fix --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "prepare": "npm run build && node lib/bin install"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn typecheck",
      "pre-commit": "yarn lint && yarn typecheck"
    }
  },
  "dependencies": {
    "@date-io/date-fns": "^2.10.11",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@turf/buffer": "^6.4.0",
    "@turf/collect": "^6.4.0",
    "@turf/turf": "^6.4.0",
    "@types/leaflet": "^1.7.3",
    "@types/react-leaflet": "^2.8.1",
    "axios": "^0.21.1",
    "date-fns": "^2.22.1",
    "leaflet-measure-path": "^1.5.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "^16.13.1",
    "react-coordinate-input": "^1.0.0-rc.2",
    "react-dom": "^16.13.1",
    "react-leaflet-box-zoom": "^1.0.2",
    "react-leaflet-control": "^2.1.2",
    "react-leaflet-coordinates": "^1.0.1",
    "react-leaflet-zoom-indicator": "^0.1.2",
    "react-redux": "^7.2.4",
    "react-scripts": "3.4.1",
    "react-toastify": "^7.0.4",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "husky": "^4.2.5",
    "node-sass": "^4.14.0",
    "prettier": "^2.0.5",
    "typescript": "~3.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}